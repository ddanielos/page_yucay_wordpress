@import './Menu.styl'

.u-wrapper
   width 100%
   height 100%
   display flex
   margin auto
   align-items center
   padding 20px 0px
.header
   width 100%
   top 0
   left 0
   right 0
   padding 0
   background-color rgba(244, 239, 226, 0.95)

@media (max-width: 1023px)
   .header
      background-color rgba(0,0,0,0.77)
      width 100%
      height 72px